class BloonType {
  int ID, hp, speed, rbe, radius;
  int[] children;
  String name;
  boolean regrow, camo;
  PImage sprite;

  BloonType(int typeID, String bName, int[] stats, int[] childrenID) {
    ID = typeID;
    name = bName;
    hp = stats[0];
    speed = stats[1];
    rbe = stats[2];
    radius = stats[3];
    regrow = stats[4] == 1;
    camo = stats[5] == 1;
    children = childrenID;
    sprite = loadImage("images/bloons/" + name + ".png");
  }
}

